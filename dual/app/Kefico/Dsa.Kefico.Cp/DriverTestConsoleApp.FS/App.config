<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <appSettings>
    <add key="upsIp" value="192.168.0.33" />
    <add key="upsPort" value="23" />
    <add key="lcrIp" value="192.168.0.51" />
    <add key="lcrPort" value="3500" />
    <add key="upsUserId" value="apc" />
    <add key="upsPassword" value="apc" />
    <add key="paixIp" value="192.168.0.12" />
    <add key="powerSupplyIp" value="192.168.0.110" />
    <add key="powerSupplyPort" value="9221" />
    <add key="pauseBetweenSendAndReceiveMilli" value="20" />
    <add key="tcpSocketConnectionTimeoutMilli" value="1000" />
    <add key="queryIntervalMilli" value="30000" />
    <add key="telnetCommunicationPauseMilli" value="200" />
  </appSettings>
  <log4net>
    <appender name="FileLogger" type="log4net.Appender.RollingFileAppender">
      <file type="log4net.Util.PatternString" value="logTestDriver-%date{yyyyMMdd}.txt" />
      <appendToFile value="true" />
      <rollingStyle value="Date" />
      <preserveLogFileNameExtension value="true" />
      <datePattern value="yyyyMMdd" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%d{yyyy.MM.dd HH:mm:ss.fff} [%thread] %-5level %message%newline" />
      </layout>
    </appender>
    <appender name="ConsoleLogger" type="log4net.Appender.ConsoleAppender">
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="= %message%newline" />
      </layout>
    </appender>
    <root>
      <level value="DEBUG" />
      <appender-ref ref="ConsoleLogger" />
    </root>
    <logger name="DriverTestLogger">
      <level value="DEBUG" />
      <appender-ref ref="FileLogger" />
    </logger>
  </log4net>
</configuration>